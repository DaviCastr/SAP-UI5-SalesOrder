{
  "_version": "1.76.0",
  "sap.app": {
    "id": "apps.dflc.salesorderts",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.1",
      "toolsId": "f7f0e486-bc81-44a8-b8b2-3b459fdcbb5b"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/Y1_SALES_ORDER_DFLC_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "SalesManagerTs-manage": {
          "semanticObject": "SalesManagerTs",
          "action": "manage",
          "title": "{{SalesManagerTs-manage.flpTitle}}",
          "subTitle": "{{SalesManagerTs-manage.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.141.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "apps.dflc.salesorderts.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "useBatch": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "apps.dflc.salesorderts.view",
        "async": true,
        "viewPath": "apps.dflc.salesorderts.view"
      },
      "routes": [
        {
          "name": "RouteOrderList",
          "pattern": ":?query:",
          "target": [
            "TargetOrderList"
          ]
        },
        {
          "name": "RouteOrderNew",
          "pattern": "SalesOrderHeader/new",
          "target": [
            "TargetOrderForm"
          ]
        },
        {
          "name": "RouteOrderEdit",
          "pattern": "SalesOrderHeader({SalesOrderID})/edit",
          "target": [
            "TargetOrderForm"
          ]
        }
      ],
      "targets": {
        "TargetOrderList": {
          "id": "OrderList",
          "name": "OrderList"
        },
        "TargetOrderForm": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "OrderForm",
          "viewName": "OrderForm"
        }
      }
    },
    "rootView": {
      "viewName": "apps.dflc.salesorderts.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "appsdflcsalesorderts"
  }
}
